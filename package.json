{
  "name": "styled-snapshot",
  "version": "0.2.3",
  "description": "A utility for taking Jest snapshots of components that use Styled Components.",
  "keywords": [
    "enzyme",
    "jest",
    "react",
    "styled-components",
    "snapshot"
  ],
  "homepage": "https://github.com/badbatch/styled-snapshot",
  "bugs": {
    "url": "https://github.com/badbatch/styled-snapshot/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/badbatch/styled-snapshot"
  },
  "license": "MIT",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn run clean:libs && yarn run compile",
    "clean": "yarn run clean:libs && yarn run clean:repo",
    "clean:libs": "del-cli ./lib",
    "clean:repo": "del-cli ./node_modules",
    "compile": "concurrently npm:compile:*",
    "compile-watch": "concurrently npm:compile-watch:*",
    "compile-watch:main": "yarn run compile:main --watch",
    "compile-watch:types": "yarn run compile:types --watch",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\",\".tsx\" --source-maps --config-file ./babel.config.js",
    "compile:module": "cross-env BABEL_ENV=module babel ./src --out-dir ./lib/module --extensions \".ts\",\".tsx\" --source-maps --config-file ./babel.config.js",
    "compile:types": "tsc --declaration --declarationMap --emitDeclarationOnly",
    "cutoff": "cutoff",
    "cutoff:post-version": "yarn run compile",
    "init": "yarn",
    "lint": "concurrently npm:lint:*",
    "lint:js": "eslint .",
    "lint:ts": "tslint --project ./tsconfig.json",
    "new-repo": "new-repo",
    "publish-cutoff": "publish-cutoff",
    "test": "jest --logHeapUsage",
    "type-check": "tsc --noEmit",
    "validate": "yarn run build && yarn run lint && yarn run type-check && yarn run test",
    "wipe": "del-cli ./yarn.lock ./node_modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "resolutions": {
    "@types/react": "16.9.9"
  },
  "dependencies": {
    "@repodog/constants": "^0.3.25",
    "@repodog/helpers": "^0.3.24",
    "enzyme-to-json": "^3.4.3",
    "md5": "^2.2.1",
    "prettier": "^1.18.2",
    "react-is": "^16.11.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "core-js": "> 4.0",
    "enzyme": "^3.10.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "react": "^16.8.0",
    "styled-components": "^4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@repodog/babel-config": "^0.3.24",
    "@repodog/build-references": "^0.3.25",
    "@repodog/commitlint-config": "^0.3.23",
    "@repodog/config-helpers": "^0.3.29",
    "@repodog/eslint-config": "^0.3.24",
    "@repodog/jest-config": "^0.3.25",
    "@repodog/lerna-config": "^0.3.23",
    "@repodog/markdownlint-config": "^0.3.23",
    "@repodog/new-package": "^0.3.24",
    "@repodog/new-repo": "^0.3.28",
    "@repodog/prettier-config": "^0.3.24",
    "@repodog/ts-config": "^0.3.23",
    "@repodog/tslint-config": "^0.3.24",
    "@repodog/typedoc-config": "^0.3.23",
    "@repodog/types": "^0.3.25",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.144",
    "@types/md5": "^2.1.33",
    "@types/prettier": "^1.18.3",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-is": "^16.7.1",
    "@types/styled-components": "^4.1.19",
    "babel-eslint": "^10.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "codecov": "^3.6.1",
    "concurrently": "^5.0.0",
    "core-js": "^3.3.3",
    "cross-env": "^6.0.3",
    "cutoff": "^0.2.7",
    "del-cli": "^3.0.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-sort-class-members": "^1.6.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lerna": "^3.18.3",
    "lodash": "^4.17.15",
    "markdownlint-cli": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.0",
    "ts-toolbelt": "^4.8.22",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "type-fest": "^0.8.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.11",
    "typescript": "^3.6.4",
    "utility-types": "^3.8.0"
  },
  "engines": {
    "node": "< 10.16.0"
  }
}
